local function checkNetworkSafety()
    print("ğŸ”’ é€²è¡Œç¶²çµ¡å®‰å…¨æª¢æŸ¥...")
    
    -- æª¢æŸ¥URLæ˜¯å¦æœ‰æ•ˆ
    local function isValidURL(url)
        return url and type(url) == "string" and 
               (url:find("^https://") or url:find("^http://"))
    end
    
    for name, url in pairs(EXTERNAL_SCRIPTS) do
        if isValidURL(url) then
            print("âœ… " .. name .. " URLæ ¼å¼æœ‰æ•ˆ: " .. url)
        else
            warn("âš ï¸ " .. name .. " URLæ ¼å¼å¯èƒ½ç„¡æ•ˆ: " .. tostring(url))
        end
    end
    
    print("ğŸ”’ ç¶²çµ¡å®‰å…¨æª¢æŸ¥å®Œæˆ")
end

checkNetworkSafety()

