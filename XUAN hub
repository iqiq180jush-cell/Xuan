    if leavingPlayer == player then
        -- æ¸…ç†æ‰€æœ‰é€£æ¥
        stopAdvancedFastAttack()
        
        -- æ¸…ç†å…¨å±€è®Šé‡
        _G.FastAttack = false
        
        print("ğŸ§¹ æ¸…ç†ç©å®¶è³‡æº...")
    end
end)

